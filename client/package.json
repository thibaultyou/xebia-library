{
  "name": "client",
  "version": "0.1.0",
  "description": "Xebia library React client",
  "homepage": "https://github.com/thibaultyou/xebia-library#readme",
  "engines": {
    "node": ">=8"
  },
  "private": true,
  "dependencies": {
    "eslint-config-react-app": "4.0.0",
    "prettier": "1.17.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.0"
  },
  "scripts": {
    "prettier": "prettier --write **/*.{js,jsx,html,md,mdx,yaml,json,css,scss,less}",
    "lint": "eslint . --cache; exit 0",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "start:dev": "npm run prettier && npm run lint && CI=true npm run test && FORCE_COLOR=true npm run start | cat"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
