{
  "name": "client",
  "version": "0.1.0",
  "description": "Xebia library React client",
  "engines": {
    "node": ">=8"
  },
  "private": true,
  "dependencies": {
    "@material-ui/core": "3.9.3",
    "@material-ui/icons": "3.0.2",
    "apollo-boost": "0.3.1",
    "apollo-cache-inmemory": "1.5.1",
    "apollo-client": "2.5.1",
    "apollo-link-http": "1.5.14",
    "graphql": "14.2.1",
    "lodash": "4.17.11",
    "react": "16.8.6",
    "react-apollo": "2.5.5",
    "react-dom": "16.8.6",
    "react-redux": "7.0.3",
    "react-router-dom": "5.0.0",
    "react-scripts": "3.0.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "styled-components": "4.2.0"
  },
  "scripts": {
    "prettier": "prettier --write src/**/*.{js,jsx,html,md,mdx,yaml,json,css,scss,less}",
    "test:snap": "jest",
    "lint": "eslint . --cache; exit 0",
    "start": "react-scripts start",
    "build": "react-scripts build",
